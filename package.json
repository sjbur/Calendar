    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "husky": "8.0.0",
    "jsdom": "^26.0.0",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2",
    "typescript-eslint": "8.24.1",
    "vite": "6.2.5",
    "vitest": "^3.1.1"
